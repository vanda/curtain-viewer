    "cross-env": "6.x",
    "css-loader": "latest",
    "eslint": "6.x",
    "eslint-loader": "4.x",
    "file-loader": "6.x",
    "gh-pages": "6.x",
    "html-webpack-plugin": "5.x",
    "mini-css-extract-plugin": "latest",
    "postcss-css-variables": "latest",
    "postcss-loader": "latest",
    "sass": "1.x",
    "sass-loader": "latest",
    "style-loader": "latest",
    "stylelint": "13.x",
